  "license": "ISC",
  "dependencies": {
    "graphql": "^0.9.0",
    "rxjs": "^5.0.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/feliperohdee/smallorange-graphql-lazy-executor"
  },
  "homepage": "https://smallorange.co"
}
